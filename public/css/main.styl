
body
    #app-header
        height 80px
    #app-header h1
        float left
        clear both
    #app-header h4
        float left
        clear both

    #app-body pre
        width 100%
        height 800px
        background rgba(247, 247, 255, 1.0)
        border 2px solid rgba(128, 128, 128, 0.6)
        overflow auto
        padding 4px 16px
    #app-body pre code
        font-family monospace
